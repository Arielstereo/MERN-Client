{
  "version": 2,
 
  "builds": [
      { "use": "@vercel/static-build", "src": "package.json"}
  ],
  "routes": [
    {
      "src": "(.*).js",
      "dest": "/$1.js"            
  },
  {
      "src": "(.*).json",
      "dest": "/$1.json"            
  },
  {
    "src": "(.*).jsx",
    "dest": "/$1.jsx"            
},
  {
      "src": "/.*",
      "dest": "index.html"
  }
  ]
} 